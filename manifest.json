{
  "name": "LinkFrame",
  "version": "0.0.1",
  "manifest_version": 2,
  "description": "An open geteway for Handshake",
  "homepage_url": "https://easychain.tech",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "default_locale": "en",
  "background": {
    "scripts": [
      "src/bg/background.js",
      "src/bg/resolver.js",
      "src/bg/tld.js"
    ],
    "persistent": true
  },
  "browser_action": {
    "default_icon": "icons/icon19.png",
    "default_title": "browser action demo",
    "default_popup": "src/browser_action/browser_action.html"
  },
  "permissions": [
    "history",
    "proxy",
    "webRequest",
    "webRequestBlocking",
    "tabs",
    "storage",
    "<all_urls>"
  ]
}